{
  "name": "divyansh-international",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm run type-check && pnpm run lint:strict && dotenv -e .env.local -- prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules/.cache",
    "analyze": "ANALYZE=true next build",
    "audit": "pnpm audit --audit-level=moderate",
    "update:all": "pnpm update --latest --recursive",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:pull": "dotenv -e .env.local -- prisma db pull",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.local -- prisma migrate deploy",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset",
    "sanity:seed": "dotenv -e .env.local -- tsx scripts/seed-sanity.ts",
    "sanity:typegen": "sanity schema extract && sanity typegen generate",
    "postinstall": "dotenv -e .env.local -- prisma generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-pdf/renderer": "^4.3.2",
    "@sanity/client": "^7.14.0",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^2.0.3",
    "@sanity/vision": "^5.5.0",
    "@supabase/supabase-js": "^2.91.0",
    "@t3-oss/env-core": "^0.13.10",
    "@t3-oss/env-nextjs": "^0.13.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.28.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "leaflet": "^1.9.4",
    "lenis": "^1.3.17",
    "lucide-react": "^0.562.0",
    "next": "^16.1.4",
    "next-sanity": "^12.0.14",
    "pg": "^8.17.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "react-leaflet": "^5.0.0",
    "react-pageflip": "^2.0.3",
    "react-pdf": "^10.3.0",
    "resend": "^6.8.0",
    "sanity": "^5.5.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.4",
    "@prisma/config": "^7.3.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/js-cookie": "^3.0.6",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/styled-components": "^5.1.36",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "styled-components": "^6.3.8",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.28.1",
  "pnpm": {
    "overrides": {
      "jsdom": "^27.4.0",
      "hono": ">=4.11.4",
      "undici": ">=6.23.0"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "@rollup/rollup-linux-x64-gnu",
      "@swc/core",
      "canvas",
      "core-js",
      "esbuild",
      "lightningcss",
      "msgpackr-extract",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  }
}
